{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "paths": {
      "components/*": [
        "HostApp/src/components/*"
      ]
    }
  },
  "include": [
    "./src",
    "./webpack.config.ts"
, "HostApp/src/services/hooks/useAuthService.tsx", "HostApp/src/context/authContext", "HostApp/src/views", "HostApp/src/routing", "HostApp/src/index.tsx", "HostApp/src/App.tsx", "HostApp/src/globalStyles.ts", "HostApp/src/components"  ]
}